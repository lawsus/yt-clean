{
    "manifest_version": 2,
    "name": "YouTube Secondary Column Remover",
    "version": "1.1",
    "description": "Removes the secondary column from YouTube watch pages",
    "permissions": [
      "https://www.youtube.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/watch*"],
        "js": ["content.js"]
      }
    ]
  }